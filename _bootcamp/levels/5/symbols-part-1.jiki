function description_to_elements with description do
  set elements to {
    "sunny": ["sun"],
    "dull": ["cloud"],
    "miserable": ["cloud", "rain"],
    "hopeful": ["sun", "cloud"],
    "rainbow-territory": ["sun", "cloud", "rain"],
    "exciting": ["cloud", "snow"],
    "snowboarding-time": ["sun", "cloud", "snow"]
  }
  return elements[description]
end
