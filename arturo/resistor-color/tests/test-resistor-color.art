import.version:3.0.0 {unitt}! 
import {src/resistor-color}!

describe "Resistor Color" [
    it "black" [
        expects.be:'equal? @[0 colorCode "black"]
    ]

    it "white" [
        expects.be:'equal? @[9 colorCode "white"]
    ]

    it "orange" [
        expects.be:'equal? @[3 colorCode "orange"]
    ]

    it "colors" [
        expects.be:'equal? @[
            ["black" "brown" "red" "orange" "yellow" "green" "blue" "violet" "grey" "white"] 
            colors
        ]
    ]
]
