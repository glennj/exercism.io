import.version:3.0.0 {unitt}! 
import {src/resistor-color-duo}!

describe "Resistor Color Duo" [
    it "brown and black returns 10" [
        expects.be:'equal? @[10 resistorValue ["brown" "black"]]
    ]

    it "blue and grey returns 68" [
        expects.be:'equal? @[68 resistorValue ["blue" "grey"]]
    ]

    it "yellow and violet returns 47" [
        expects.be:'equal? @[47 resistorValue ["yellow" "violet"]]
    ]

    it "white and red returns 92" [
        expects.be:'equal? @[92 resistorValue ["white" "red"]]
    ]

    it "orange and orange returns 33" [
        expects.be:'equal? @[33 resistorValue ["orange" "orange"]]
    ]

    it "resistorValue ignores additional colors" [
        expects.be:'equal? @[51 resistorValue ["green" "brown" "orange"]]
    ]

    it "black and brown returns a single digit" [
        expects.be:'equal? @[1 resistorValue ["black" "brown"]]
    ]
]
