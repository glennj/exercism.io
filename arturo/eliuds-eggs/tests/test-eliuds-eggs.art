import.version:3.0.0 {unitt}! 
import {src/eliuds-eggs}!

describe "Eliud's Eggs" [
    it "0 eggs" [
        expects.be:'equal? @[0 eggCount 0]
    ]

    it "1 egg" [
        expects.be:'equal? @[1 eggCount 16]
    ]

    it "4 eggs" [
        expects.be:'equal? @[4 eggCount 89]
    ]

    it "13 eggs" [
        expects.be:'equal? @[13 eggCount 2000000000]
    ]
]
