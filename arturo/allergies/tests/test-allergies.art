import.version:2.0.1 {unitt}!
import {src/allergies}!

suite "Allergies" [
  suite "Allergies - Eggs" [
    test "not allergic to anything" [
        score: to :allergies [0]
        result: do [score\allergicTo? "eggs"]
        assert -> false? result
    ]

    test "allergic only to eggs" [
        score: to :allergies [1]
        result: do [score\allergicTo? "eggs"]
        assert -> true? result
    ]

    test "allergic to eggs and something else" [
        score: to :allergies [3]
        result: do [score\allergicTo? "eggs"]
        assert -> true? result
    ]

    test "allergic to something but not eggs" [
        score: to :allergies [2]
        result: do [score\allergicTo? "eggs"]
        assert -> false? result
    ]

    test "allergic to everything" [
        score: to :allergies [255]
        result: do [score\allergicTo? "eggs"]
        assert -> true? result
    ]
  ]

