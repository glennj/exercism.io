import.version:3.0.0 {unitt}! 
import {src/square-root}!

describe "Square Root"[
    it "root of 1" [
        expects.be:'equal? @[1 squareRoot 1]
    ]

    it "root of 4" [
        expects.be:'equal? @[2 squareRoot 4]
    ]

    it "root of 25" [
        expects.be:'equal? @[5 squareRoot 25]
    ]

    it "root of 81" [
        expects.be:'equal? @[9 squareRoot 81]
    ]

    it "root of 196" [
        expects.be:'equal? @[14 squareRoot 196]
    ]

    it "root of 65025" [
        expects.be:'equal? @[255 squareRoot 65025]
    ]
]
