paired?: function [str][
    if empty? str -> return true

    brackets: #[ ")":"(" "]":"[" "}":"{" ]
    myStack: []

    loop str 'char [
        c: to :string char

        when [
            [in? c brackets][
                prepend 'myStack c
            ]

            [in? c keys brackets][
                if or? [empty? myStack] [(get brackets c) <> (first myStack)]
                    -> return false

                drop 'myStack
            ]
        ]
    ]

    empty? myStack
]
