TestCase "no primes under two"
	MOVE 1 TO WS-LIMIT
	PERFORM SIEVE
	EXPECT WS-COUNT = 0

TestCase "find first prime"
	MOVE 2 TO WS-LIMIT
	PERFORM SIEVE
	EXPECT WS-RESULT(1) = 002

TestCase "find primes up to 10"
	MOVE 10 TO WS-LIMIT
	PERFORM SIEVE
	EXPECT WS-RESULT(1) = 002
	EXPECT WS-RESULT(2) = 003
	EXPECT WS-RESULT(3) = 005
	EXPECT WS-RESULT(4) = 007

TestCase "limit is prime"
	MOVE 13 TO WS-LIMIT
	PERFORM SIEVE
	EXPECT WS-RESULT(1) = 002
	EXPECT WS-RESULT(2) = 003
	EXPECT WS-RESULT(3) = 005
	EXPECT WS-RESULT(4) = 007
	EXPECT WS-RESULT(5) = 011
	EXPECT WS-RESULT(6) = 013

